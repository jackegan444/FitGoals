<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jegan.fitgoals">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RacePredictor"></activity>
        <activity
            android:name=".EditRun"
            android:label="@string/edit_run_title"
            android:parentActivityName=".RunDetails" />
        <activity
            android:name=".RunDetails"
            android:parentActivityName=".PreviousRuns"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PreviousRuns"
            android:label="@string/prev_runs_title"
            android:parentActivityName=".RunningHome" />
        <activity
            android:name=".LogARun"
            android:label="@string/log_run_title"
            android:parentActivityName=".RunningHome" />
        <activity
            android:name=".StrengthHome"
            android:label="@string/strength_home_title"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RunningHome"
            android:label="@string/run_home_title"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>